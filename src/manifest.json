{
  "manifest_version": 3,
  "name": "Youtube AD Escape",
  "icons": {
    "512": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "logo.png"
      ],
      "matches": []
    }
  ]
}